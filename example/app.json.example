{
  "expo": {
    "name": "ExpoAnalytics Example",
    "slug": "expo-analytics-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.expo.analytics.example",
      "infoPlist": {
        "UIBackgroundModes": [
          "processing",
          "fetch",
          "background-app-refresh"
        ],
        "BGTaskSchedulerPermittedIdentifiers": [
          "com.expo.analytics.upload",
          "com.expo.analytics.sync"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSAllowsLocalNetworking": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.yourcompany.expo.analytics.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "12.0"
          }
        }
      ]
    ]
  }
} 